{
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "elm",
    "go",
    "haskell",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "prisma",
    "rust",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "tsserver.enable": true,
  "tsserver.log": "normal",
  "languageserver": {
    "kotlin": {
      "command": "/Users/robert.jewell/bin/kotlin-ls-persistent",
      "filetypes": ["kotlin"],
      "rootPatterns": ["settings.gradle.kts", "build.gradle.kts", ".git"],
      "trace.server": "verbose",
      "initializationOptions": {
        "storagePath": "/tmp/kotlin-language-server-cache"
      }
    }
  }
}
